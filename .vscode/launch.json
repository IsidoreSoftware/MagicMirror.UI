{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "outFiles": [
                "${workspaceRoot}/dist/**/*.js"
            ],
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/dist/electron.js",
            "stopOnEntry": false,
            "args": [],
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron",
            "preLaunchTask":"tsc",
            "runtimeArgs": [
                "--enable-logging",
                "--serve"
            ],
            "env": {},
            "sourceMaps": true
        },
        {
            "name": "Attach",
            "type": "node",
            "request": "attach",
            "port": 5858,
            "sourceMaps": true,
            "outFiles": []
        }
    ]
}